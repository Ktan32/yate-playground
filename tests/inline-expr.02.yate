/// {
///     description: 'strict comparison',
///     data: {
///         a: 0,
///         b: '',
///         c: false,
///         d: 1,
///         e: "foo",
///         f: true
///     },
///     result: '000001'
/// }

match / {
    if .a === true() {
        "1"
    } else {
        "0"
    }

    if .b === true() {
        "1"
    } else {
        "0"
    }

    if .c === true() {
        "1"
    } else {
        "0"
    }

    if .d === true() {
        "1"
    } else {
        "0"
    }

    if .e === true() {
        "1"
    } else {
        "0"
    }

    if .f === true() {
        "1"
    } else {
        "0"
    }
}

